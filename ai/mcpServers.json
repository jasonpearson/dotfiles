{
  "nativeMCPServers": {
    "mcphub": {
      "disabled": false,
      "disabled_tools": [
        
      ]
    }
  },
  "env": {
    "SECRET_DATABASE_URL": null
  },
  "mcpServers": {
    "github.com/modelcontextprotocol/servers/tree/main/src/postgres": {
